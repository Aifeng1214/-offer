http://www.lintcode.com/zh-cn/problem/fibonacci/

class Solution{
public:
    /**
     * @param n: an integer
     * @return an integer f(n)
     */
    int fibonacci(int n) {
        // write your code here
        if(n==1) return 0;
        if(n==2) return 1;
        int p=0,q=1;
        for(int i = 2; i < n; i++)
        {
            int tmp = p;
            p = q;
            q = tmp + q;
        }
        return q;
    }
};
