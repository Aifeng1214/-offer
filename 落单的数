http://www.lintcode.com/zh-cn/problem/single-number/

class Solution {
public:
	/**
	 * @param A: Array of integers.
	 * return: The single number.
	 */
    int singleNumber(vector<int> &A) {
        // write your code here
        int single=0;
        for(int i=0;i<A.size();++i)
            single^=A[i];
        return single;
    }
};
